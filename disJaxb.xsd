<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="pduContainer" type="pduContainer"/>

  <xs:complexType name="acknowledgePdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementFamilyPdu">
        <xs:sequence/>
        <xs:attribute name="acknowledgeFlag" type="xs:int" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
        <xs:attribute name="responseFlag" type="xs:int" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="simulationManagementFamilyPdu">
    <xs:complexContent>
      <xs:extension base="pdu">
        <xs:sequence>
          <xs:element name="fk_originatingEntityID" type="xs:long"/>
          <xs:element name="fk_receivingEntityID" type="xs:long"/>
          <xs:element name="originatingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="receivingEntityID" type="entityID" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="pdu">
    <xs:sequence>
      <xs:element name="pk_Pdu" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="exerciseID" type="xs:short" use="required"/>
    <xs:attribute name="padding" type="xs:short" use="required"/>
    <xs:attribute name="pduLength" type="xs:int" use="required"/>
    <xs:attribute name="pduType" type="xs:short" use="required"/>
    <xs:attribute name="protocolFamily" type="xs:short" use="required"/>
    <xs:attribute name="protocolVersion" type="xs:short" use="required"/>
    <xs:attribute name="timestamp" type="xs:long" use="required"/>
  </xs:complexType>

  <xs:complexType name="entityID">
    <xs:sequence>
      <xs:element name="pk_EntityID" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="application" type="xs:int" use="required"/>
    <xs:attribute name="entity" type="xs:int" use="required"/>
    <xs:attribute name="site" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="acknowledgeReliablePdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementWithReliabilityFamilyPdu">
        <xs:sequence/>
        <xs:attribute name="acknowledgeFlag" type="xs:int" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
        <xs:attribute name="responseFlag" type="xs:int" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="simulationManagementWithReliabilityFamilyPdu">
    <xs:complexContent>
      <xs:extension base="pdu">
        <xs:sequence>
          <xs:element name="fk_originatingEntityID" type="xs:long"/>
          <xs:element name="fk_receivingEntityID" type="xs:long"/>
          <xs:element name="originatingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="receivingEntityID" type="entityID" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="acousticBeamData">
    <xs:sequence>
      <xs:element name="fk_fundamentalDataParameters" type="xs:long"/>
      <xs:element name="fundamentalDataParameters" type="acousticBeamFundamentalParameter" minOccurs="0"/>
      <xs:element name="pk_AcousticBeamData" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="beamDataLength" type="xs:int" use="required"/>
    <xs:attribute name="beamIDNumber" type="xs:short" use="required"/>
    <xs:attribute name="pad2" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="acousticBeamFundamentalParameter">
    <xs:sequence>
      <xs:element name="pk_AcousticBeamFundamentalParameter" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="activeEmissionParameterIndex" type="xs:int" use="required"/>
    <xs:attribute name="azimuthalBeamwidth" type="xs:float" use="required"/>
    <xs:attribute name="beamCenterAzimuth" type="xs:float" use="required"/>
    <xs:attribute name="beamCenterDE" type="xs:float" use="required"/>
    <xs:attribute name="deBeamwidth" type="xs:float" use="required"/>
    <xs:attribute name="scanPattern" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="acousticEmitter">
    <xs:sequence>
      <xs:element name="pk_AcousticEmitter" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="acousticIdNumber" type="xs:short" use="required"/>
    <xs:attribute name="acousticName" type="xs:int" use="required"/>
    <xs:attribute name="function" type="xs:short" use="required"/>
  </xs:complexType>

  <xs:complexType name="acousticEmitterSystem">
    <xs:sequence>
      <xs:element name="pk_AcousticEmitterSystem" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="acousticFunction" type="xs:short" use="required"/>
    <xs:attribute name="acousticID" type="xs:short" use="required"/>
    <xs:attribute name="acousticName" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="acousticEmitterSystemData">
    <xs:sequence>
      <xs:element name="fk_acousticEmitterSystem" type="xs:long"/>
      <xs:element name="fk_emitterLocation" type="xs:long"/>
      <xs:element name="acousticEmitterSystem" type="acousticEmitterSystem" minOccurs="0"/>
      <xs:element name="beamRecordsList" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="beamRecords" type="acousticBeamData" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="emitterLocation" type="vector3Float" minOccurs="0"/>
      <xs:element name="pk_AcousticEmitterSystemData" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="emitterSystemDataLength" type="xs:short" use="required"/>
    <xs:attribute name="numberOfBeams" type="xs:short" use="required"/>
    <xs:attribute name="pad2" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="vector3Float">
    <xs:sequence>
      <xs:element name="pk_Vector3Float" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="x" type="xs:float" use="required"/>
    <xs:attribute name="y" type="xs:float" use="required"/>
    <xs:attribute name="z" type="xs:float" use="required"/>
  </xs:complexType>

  <xs:complexType name="actionRequestPdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementFamilyPdu">
        <xs:sequence>
          <xs:element name="fixedDatumsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="fixedDatums" type="fixedDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="variableDatumsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="variableDatums" type="variableDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="actionID" type="xs:long" use="required"/>
        <xs:attribute name="numberOfFixedDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="numberOfVariableDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="fixedDatum">
    <xs:sequence>
      <xs:element name="pk_FixedDatum" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="fixedDatumID" type="xs:long" use="required"/>
    <xs:attribute name="fixedDatumValue" type="xs:long" use="required"/>
  </xs:complexType>

  <xs:complexType name="variableDatum">
    <xs:sequence>
      <xs:element name="pk_VariableDatum" type="xs:long"/>
      <xs:element name="variableData" type="xs:base64Binary" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="variableDatumID" type="xs:long" use="required"/>
    <xs:attribute name="variableDatumLength" type="xs:long" use="required"/>
  </xs:complexType>

  <xs:complexType name="actionRequestReliablePdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementWithReliabilityFamilyPdu">
        <xs:sequence>
          <xs:element name="fixedDatumRecordsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="fixedDatumRecords" type="fixedDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="variableDatumRecordsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="variableDatumRecords" type="variableDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="actionID" type="xs:long" use="required"/>
        <xs:attribute name="numberOfFixedDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="numberOfVariableDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="pad1" type="xs:int" use="required"/>
        <xs:attribute name="pad2" type="xs:short" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
        <xs:attribute name="requiredReliabilityService" type="xs:short" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="actionResponsePdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementFamilyPdu">
        <xs:sequence>
          <xs:element name="fixedDatumsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="fixedDatums" type="fixedDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="variableDatumsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="variableDatums" type="variableDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="numberOfFixedDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="numberOfVariableDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
        <xs:attribute name="requestStatus" type="xs:long" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="actionResponseReliablePdu">
    <xs:complexContent>
      <xs:extension base="simulationManagementWithReliabilityFamilyPdu">
        <xs:sequence>
          <xs:element name="fixedDatumRecordsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="fixedDatumRecords" type="fixedDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="variableDatumRecordsList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="variableDatumRecords" type="variableDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="numberOfFixedDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="numberOfVariableDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="requestID" type="xs:long" use="required"/>
        <xs:attribute name="responseStatus" type="xs:long" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="aggregateID">
    <xs:sequence>
      <xs:element name="pk_AggregateID" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="aggregateID" type="xs:int" use="required"/>
    <xs:attribute name="application" type="xs:int" use="required"/>
    <xs:attribute name="site" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="aggregateMarking">
    <xs:sequence>
      <xs:element name="characters" type="xs:base64Binary" minOccurs="0"/>
      <xs:element name="pk_AggregateMarking" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="characterSet" type="xs:short" use="required"/>
  </xs:complexType>

  <xs:complexType name="aggregateStatePdu">
    <xs:complexContent>
      <xs:extension base="entityManagementFamilyPdu">
        <xs:sequence>
          <xs:element name="fk_aggregateID" type="xs:long"/>
          <xs:element name="fk_aggregateType" type="xs:long"/>
          <xs:element name="fk_aggregateMarking" type="xs:long"/>
          <xs:element name="fk_dimensions" type="xs:long"/>
          <xs:element name="fk_orientation" type="xs:long"/>
          <xs:element name="fk_centerOfMass" type="xs:long"/>
          <xs:element name="fk_velocity" type="xs:long"/>
          <xs:element name="aggregateID" type="entityID" minOccurs="0"/>
          <xs:element name="aggregateIDListList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="aggregateIDList" type="aggregateID" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="aggregateMarking" type="aggregateMarking" minOccurs="0"/>
          <xs:element name="aggregateType" type="entityType" minOccurs="0"/>
          <xs:element name="centerOfMass" type="vector3Double" minOccurs="0"/>
          <xs:element name="dimensions" type="vector3Float" minOccurs="0"/>
          <xs:element name="entityIDListList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="entityIDList" type="entityID" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="orientation" type="orientation" minOccurs="0"/>
          <xs:element name="silentAggregateSystemListList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="silentAggregateSystemList" type="entityType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="silentEntitySystemListList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="silentEntitySystemList" type="entityType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="variableDatumListList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="variableDatumList" type="variableDatum" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="velocity" type="vector3Float" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="aggregateState" type="xs:short" use="required"/>
        <xs:attribute name="forceID" type="xs:short" use="required"/>
        <xs:attribute name="formation" type="xs:long" use="required"/>
        <xs:attribute name="numberOfDisAggregates" type="xs:int" use="required"/>
        <xs:attribute name="numberOfDisEntities" type="xs:int" use="required"/>
        <xs:attribute name="numberOfSilentAggregateTypes" type="xs:int" use="required"/>
        <xs:attribute name="numberOfSilentEntityTypes" type="xs:int" use="required"/>
        <xs:attribute name="numberOfVariableDatumRecords" type="xs:long" use="required"/>
        <xs:attribute name="pad2" type="xs:short" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="entityManagementFamilyPdu">
    <xs:complexContent>
      <xs:extension base="pdu">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="entityType">
    <xs:sequence>
      <xs:element name="pk_EntityType" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="category" type="xs:short" use="required"/>
    <xs:attribute name="country" type="xs:int" use="required"/>
    <xs:attribute name="domain" type="xs:short" use="required"/>
    <xs:attribute name="entityKind" type="xs:short" use="required"/>
    <xs:attribute name="extra" type="xs:short" use="required"/>
    <xs:attribute name="spec" type="xs:short" use="required"/>
    <xs:attribute name="subcategory" type="xs:short" use="required"/>
  </xs:complexType>

  <xs:complexType name="vector3Double">
    <xs:sequence>
      <xs:element name="pk_Vector3Double" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="x" type="xs:double" use="required"/>
    <xs:attribute name="y" type="xs:double" use="required"/>
    <xs:attribute name="z" type="xs:double" use="required"/>
  </xs:complexType>

  <xs:complexType name="orientation">
    <xs:sequence>
      <xs:element name="pk_Orientation" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="phi" type="xs:float" use="required"/>
    <xs:attribute name="psi" type="xs:float" use="required"/>
    <xs:attribute name="theta" type="xs:float" use="required"/>
  </xs:complexType>

  <xs:complexType name="aggregateType">
    <xs:sequence>
      <xs:element name="pk_AggregateType" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="aggregateKind" type="xs:short" use="required"/>
    <xs:attribute name="category" type="xs:short" use="required"/>
    <xs:attribute name="country" type="xs:int" use="required"/>
    <xs:attribute name="domain" type="xs:short" use="required"/>
    <xs:attribute name="extra" type="xs:short" use="required"/>
    <xs:attribute name="specificInfo" type="xs:short" use="required"/>
    <xs:attribute name="subcategory" type="xs:short" use="required"/>
  </xs:complexType>

  <xs:complexType name="angularVelocityVector">
    <xs:sequence>
      <xs:element name="pk_AngularVelocityVector" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="x" type="xs:float" use="required"/>
    <xs:attribute name="y" type="xs:float" use="required"/>
    <xs:attribute name="z" type="xs:float" use="required"/>
  </xs:complexType>

  <xs:complexType name="antennaLocation">
    <xs:sequence>
      <xs:element name="fk_antennaLocation" type="xs:long"/>
      <xs:element name="fk_relativeAntennaLocation" type="xs:long"/>
      <xs:element name="antennaLocation" type="vector3Double" minOccurs="0"/>
      <xs:element name="pk_AntennaLocation" type="xs:long"/>
      <xs:element name="relativeAntennaLocation" type="vector3Float" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="apaData">
    <xs:sequence>
      <xs:element name="pk_ApaData" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="parameterIndex" type="xs:int" use="required"/>
    <xs:attribute name="parameterValue" type="xs:short" use="required"/>
  </xs:complexType>

  <xs:complexType name="arealObjectStatePdu">
    <xs:complexContent>
      <xs:extension base="syntheticEnvironmentFamilyPdu">
        <xs:sequence>
          <xs:element name="fk_objectID" type="xs:long"/>
          <xs:element name="fk_referencedObjectID" type="xs:long"/>
          <xs:element name="fk_objectType" type="xs:long"/>
          <xs:element name="fk_objectAppearance" type="xs:long"/>
          <xs:element name="fk_requesterID" type="xs:long"/>
          <xs:element name="fk_receivingID" type="xs:long"/>
          <xs:element name="objectAppearance" type="sixByteChunk" minOccurs="0"/>
          <xs:element name="objectID" type="entityID" minOccurs="0"/>
          <xs:element name="objectLocationList" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="objectLocation" type="vector3Double" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="objectType" type="entityType" minOccurs="0"/>
          <xs:element name="receivingID" type="simulationAddress" minOccurs="0"/>
          <xs:element name="referencedObjectID" type="entityID" minOccurs="0"/>
          <xs:element name="requesterID" type="simulationAddress" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="forceID" type="xs:short" use="required"/>
        <xs:attribute name="modifications" type="xs:short" use="required"/>
        <xs:attribute name="numberOfPoints" type="xs:int" use="required"/>
        <xs:attribute name="updateNumber" type="xs:int" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="syntheticEnvironmentFamilyPdu">
    <xs:complexContent>
      <xs:extension base="pdu">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="sixByteChunk">
    <xs:sequence>
      <xs:element name="otherParameters" type="xs:base64Binary" minOccurs="0"/>
      <xs:element name="pk_SixByteChunk" type="xs:long"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="simulationAddress">
    <xs:sequence>
      <xs:element name="pk_SimulationAddress" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="application" type="xs:int" use="required"/>
    <xs:attribute name="site" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="articulationParameter">
    <xs:sequence>
      <xs:element name="pk_ArticulationParameter" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="changeIndicator" type="xs:short" use="required"/>
    <xs:attribute name="parameterType" type="xs:int" use="required"/>
    <xs:attribute name="parameterTypeDesignator" type="xs:short" use="required"/>
    <xs:attribute name="parameterValue" type="xs:double" use="required"/>
    <xs:attribute name="partAttachedTo" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="beamAntennaPattern">
    <xs:sequence>
      <xs:element name="fk_beamDirection" type="xs:long"/>
      <xs:element name="beamDirection" type="orientation" minOccurs="0"/>
      <xs:element name="pk_BeamAntennaPattern" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="azimuthBeamwidth" type="xs:float" use="required"/>
    <xs:attribute name="elevationBeamwidth" type="xs:float" use="required"/>
    <xs:attribute name="ex" type="xs:float" use="required"/>
    <xs:attribute name="ez" type="xs:float" use="required"/>
    <xs:attribute name="padding1" type="xs:short" use="required"/>
    <xs:attribute name="padding2" type="xs:byte" use="required"/>
    <xs:attribute name="phase" type="xs:float" use="required"/>
    <xs:attribute name="referenceSystem" type="xs:float" use="required"/>
  </xs:complexType>

  <xs:complexType name="beamData">
    <xs:sequence>
      <xs:element name="pk_BeamData" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="beamAzimuthCenter" type="xs:float" use="required"/>
    <xs:attribute name="beamAzimuthSweep" type="xs:float" use="required"/>
    <xs:attribute name="beamElevationCenter" type="xs:float" use="required"/>
    <xs:attribute name="beamElevationSweep" type="xs:float" use="required"/>
    <xs:attribute name="beamSweepSync" type="xs:float" use="required"/>
  </xs:complexType>

  <xs:complexType name="burstDescriptor">
    <xs:sequence>
      <xs:element name="fk_munition" type="xs:long"/>
      <xs:element name="munition" type="entityType" minOccurs="0"/>
      <xs:element name="pk_BurstDescriptor" type="xs:long"/>
    </xs:sequence>
    <xs:attribute name="fuse" type="xs:int" use="required"/>
    <xs:attribute name="quantity" type="xs:int" use="required"/>
    <xs:attribute name="rate" type="xs:int" use="required"/>
    <xs:attribute name="warhead" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="clockTime">
    <xs:sequence>
